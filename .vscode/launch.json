  "version": "0.2.0",
  "configurations": [
    {
      "name": "gitspaces create:https",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "/Users/davidmay/code/testarea",
      "args": [
        "--wrapid", "0",
        "create",
        "https://github.com/davfive/gitspaces.git",
        "-n",
        "3"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "gitspaces create:ssh",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "/Users/davidmay/code/testarea",
      "args": [
        "--wrapid", "0",
        "create",
        "git@github.com:davfive/gitspaces.git",
        "-n",
        "3"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "gitspaces switch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "args": ["--wrapid", "0", "switch"],
      "console": "integratedTerminal"
    },
    {
      "name": "gitspaces setup",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "args": ["setup"],
      "console": "integratedTerminal"
    },
    {
      "name": "gitspaces -v",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "args": ["-v"],
      "console": "integratedTerminal"
    },
    {
      "name": "gitspaces -h",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "args": ["-h"],
      "console": "integratedTerminal"
    },
    {
      "name": "gitspaces --wrapid=0 <empty>",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "args": ["--wrapid", "0"],
      "console": "integratedTerminal"
    },
    {
      "name": "gitspaces <empty>",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "args": [],
      "console": "integratedTerminal"
    }
  ]
}
